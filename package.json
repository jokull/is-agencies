{
  "name": "is-agencies",
  "description": "Community maintained site of Icelandic web agencies",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev --compatibility-date=2024-12-01 -- vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "lint": "oxlint -c .config/.oxlintrc.json --type-aware --type-check --deny-warnings --report-unused-disable-directives",
    "fmt": "oxfmt -c .config/.oxfmtrc.json",
    "fmt:check": "oxfmt -c .config/.oxfmtrc.json --check",
    "deploy": "wrangler pages deploy .svelte-kit/cloudflare"
  },
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "7.2.4",
    "@sveltejs/kit": "2.49.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251205.0",
    "@sveltejs/vite-plugin-svelte": "5.1.1",
    "@tailwindcss/vite": "4.1.17",
    "@types/node": "24.10.1",
    "gh-pages": "6.3.0",
    "oxfmt": "0.15.0",
    "oxlint": "1.31.0",
    "oxlint-tsgolint": "0.8.3",
    "svelte": "5.45.5",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "wrangler": "4.53.0"
  }
}
